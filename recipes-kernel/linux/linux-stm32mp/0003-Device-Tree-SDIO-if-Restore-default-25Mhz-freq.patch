From 9919ead01102c9d24dc21bd796ec51d57647c76d Mon Sep 17 00:00:00 2001
From: Stephen Livingston <stephen.livingston@hlioengineering.com>
Date: Tue, 14 Sep 2021 17:01:29 -0400
Subject: [PATCH 1/3] Device Tree SDIO i/f: Restore default 25Mhz sdio freq.
 Set st.neg-edge.

---
 arch/arm/boot/dts/stm32mp15xx-hlio-rcb.dtsi | 5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/stm32mp15xx-hlio-rcb.dtsi b/arch/arm/boot/dts/stm32mp15xx-hlio-rcb.dtsi
index e75383f59..d1318db57 100644
--- a/arch/arm/boot/dts/stm32mp15xx-hlio-rcb.dtsi
+++ b/arch/arm/boot/dts/stm32mp15xx-hlio-rcb.dtsi
@@ -8,14 +8,15 @@
 
 /* ESP32 Module SDIO */
 &sdmmc3 {
+//		arm,primecell-periphid = <0x10153180>;
 		pinctrl-names = "default", "opendrain", "sleep";
 		pinctrl-0 = <&sdmmc3_pins_mx>;
 		pinctrl-1 = <&sdmmc3_opendrain_pins_mx>;
 		pinctrl-2 = <&sdmmc3_sleep_pins_mx>;
 		bus-width = <4>;
 		vmmc-supply = <&v3v3>;
-		max-frequency = <400000>;
-//		st,neg-edge;
+//		max-frequency = <400000>;
+		st,neg-edge;
 //		non-removable;
 //		cap-sdio-irq;
 //		interrupts-extended = <&exti ?? IRQ_TYPE_LEVEL_HIGH>;
